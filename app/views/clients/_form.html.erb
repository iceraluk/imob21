<%= form_for(@client, :html => {:multipart => true, :class => "form-horizontal"} ) do |f| %>

    <div class="input-group">
      <%= f.label :titlu, :class => "input-group-addon" %>
      <%= f.text_field :titlu, :class => "form-control" %>
    </div>

    <div class="input-group">
      <%= f.label :tip_cerere, :class => "input-group-addon" %>
      <%= f.text_field :tip_cerere, :class => "form-control" %>
    </div>

    <div class="input-group">
      <%= f.label :judet, :class => "input-group-addon" %>
      <%= f.text_field :judet, :class => "form-control" %>
    </div>

    <div class="input-group">
      <%= f.label :oras, :class => "input-group-addon" %>
      <%= f.text_field :oras, :class => "form-control" %>
    </div>

    <div class="input-group">
      <%= f.label :cartier, :class => "input-group-addon" %>
      <%= f.text_field :cartier, :class => "form-control" %>
    </div>

    <div class="input-group">
      <%= f.label :zone, :class => "input-group-addon" %>
      <%= f.text_field :zone, :class => "form-control" %>
    </div>

    <div class="input-group">
      <%= f.label :data_limita, :class => "input-group-addon" %>
      <%= f.text_field :data_limita, :class => "form-control" %>
    </div>


    <%= f.submit "Save", :class => "btn btn-default", :disable_with => "Processing" %>
    <%= link_to "Cancel", :back, :class => "btn btn-default" %>
<% end %>